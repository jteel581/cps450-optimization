class math is
	add(x: int; y: int) : int is
	result: int
	begin
		result := x + y
		add := result
	end add
	oneThat(a: int) : int is
	begin
		if a = 1 then
			oneThat := a
		else
			oneThat := oneThat(a - 1)
		end if
	end oneThat
	start() is
	result: int
	begin
		result := oneThat(10)
		writeint(result)
	end start
end math
		